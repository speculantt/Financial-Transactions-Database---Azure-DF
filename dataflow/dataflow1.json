{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "File_import",
						"type": "DatasetReference"
					},
					"name": "NewFile",
					"description": "financial import from santander"
				},
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "csvdatabasefile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "Source",
					"description": "Adding source column"
				},
				{
					"name": "Source1",
					"description": "Adding source column"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {FECHA OPERACIÓN} as string,",
				"          {FECHA VALOR} as string,",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as string,",
				"          SALDO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['NewFilesUpload','Processed']) ~> NewFile",
				"source(output(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csvdatabasefile",
				"csvdatabasefile derive(Source = 'existing') ~> Source",
				"select1 derive(Source = 'NewFile') ~> Source1",
				"Source1 derive(Key = crc32(concat(DateOperation,DateEffective,Amount,Saldo))) ~> derivedColumn1",
				"NewFile cast(output(",
				"          {FECHA OPERACIÓN} as date 'yyyy-MM-dd',",
				"          {FECHA VALOR} as date 'yyyy-MM-dd',",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as double,",
				"          SALDO as double",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 select(mapColumn(",
				"          DateOperation = {FECHA OPERACIÓN},",
				"          DateEffective = {FECHA VALOR},",
				"          Description = CONCEPTO,",
				"          Amount = {IMPORTE EUR},",
				"          Saldo = SALDO",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"Source sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}