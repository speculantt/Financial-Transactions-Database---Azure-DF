{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "File_import",
						"type": "DatasetReference"
					},
					"name": "NewFile",
					"description": "financial import from santander"
				},
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "csvdatabasefile"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "AddSource1",
					"description": "Adding source column"
				},
				{
					"name": "AddSource",
					"description": "Adding source column"
				},
				{
					"name": "AddKey"
				},
				{
					"name": "FilterNulls"
				},
				{
					"name": "AddKey1"
				},
				{
					"name": "union1"
				},
				{
					"name": "RenameColumns"
				},
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {FECHA OPERACIÃ“N} as string,",
				"          {FECHA VALOR} as string,",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as string,",
				"          SALDO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['NewFilesUpload','Processed']) ~> NewFile",
				"source(output(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csvdatabasefile",
				"csvdatabasefile derive(Source = 'existing') ~> AddSource1",
				"NewFile derive(Source = 'NewFile') ~> AddSource",
				"AddSource derive(Key = crc32(concat({FECHA OPERACIÃ“N},{FECHA VALOR},{IMPORTE EUR},{SALDO}))) ~> AddKey",
				"AddKey filter(or(not(isNull({FECHA OPERACIÃ“N})),not(isNull({IMPORTE EUR})))) ~> FilterNulls",
				"AddSource1 derive(Key = crc32(concat(DateOperation,DateEffective,Amount,Saldo))) ~> AddKey1",
				"AddKey1, RenameColumns union(byName: true)~> union1",
				"FilterNulls select(mapColumn(",
				"          DateOperation = {FECHA OPERACIÃ“N},",
				"          DateEffective = {FECHA VALOR},",
				"          Description = CONCEPTO,",
				"          Amount = {IMPORTE EUR},",
				"          Saldo = SALDO,",
				"          Source,",
				"          Key",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenameColumns",
				"union1 split(ERROR_FUNCTION(''),",
				"     disjoint: false) ~> split1@(, )"
			]
		}
	}
}