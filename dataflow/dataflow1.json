{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "File_import",
						"type": "DatasetReference"
					},
					"name": "NewFile",
					"description": "financial import from santander"
				},
				{
					"name": "FullDatabaseFile",
					"description": "financial import from santander"
				},
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "csvdatabasefile"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "Source",
					"description": "Adding source column"
				},
				{
					"name": "Source1",
					"description": "Adding source column"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {FECHA OPERACIÃ“N} as string,",
				"          {FECHA VALOR} as string,",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as string,",
				"          SALDO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['NewFilesUpload','Processed']) ~> NewFile",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> FullDatabaseFile",
				"source(output(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csvdatabasefile",
				"FullDatabaseFile derive(Source = 'existing') ~> Source",
				"NewFile derive(Source = 'NewFile') ~> Source1",
				"Source sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}