{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "File_import",
						"type": "DatasetReference"
					},
					"name": "NewFile",
					"description": "financial import from santander"
				},
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "csvdatabasefile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "Source",
					"description": "Adding source column"
				},
				{
					"name": "AddSource",
					"description": "Adding source column"
				},
				{
					"name": "AddKey"
				},
				{
					"name": "FilterNulls"
				},
				{
					"name": "rank1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {FECHA OPERACIÓN} as string,",
				"          {FECHA VALOR} as string,",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as string,",
				"          SALDO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['NewFilesUpload','Processed']) ~> NewFile",
				"source(output(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csvdatabasefile",
				"csvdatabasefile derive(Source = 'existing') ~> Source",
				"NewFile derive(Source = 'NewFile') ~> AddSource",
				"AddSource derive(Key = crc32(concat({FECHA OPERACIÓN},{FECHA VALOR},{IMPORTE EUR},{SALDO}))) ~> AddKey",
				"AddKey filter(or(not(isNull({FECHA OPERACIÓN})),not(isNull({IMPORTE EUR})))) ~> FilterNulls",
				"FilterNulls rank(asc(Key, true),",
				"     output(Rank as long)) ~> rank1",
				"Source sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}