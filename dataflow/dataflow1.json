{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "File_import",
						"type": "DatasetReference"
					},
					"name": "NewFile",
					"description": "financial import from santander"
				},
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "csvdatabasefile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "fulldatabasefile",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "Source",
					"description": "Adding source column"
				},
				{
					"name": "AddSource",
					"description": "Adding source column"
				},
				{
					"name": "FormatColumns"
				},
				{
					"name": "RenameColumns"
				},
				{
					"name": "AddKey"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {FECHA OPERACIÓN} as string,",
				"          {FECHA VALOR} as string,",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as string,",
				"          SALDO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['NewFilesUpload','Processed']) ~> NewFile",
				"source(output(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csvdatabasefile",
				"csvdatabasefile derive(Source = 'existing') ~> Source",
				"RenameColumns derive(Source = 'NewFile') ~> AddSource",
				"NewFile cast(output(",
				"          {FECHA OPERACIÓN} as date 'yyyy-MM-dd',",
				"          {FECHA VALOR} as date 'yyyy-MM-dd',",
				"          CONCEPTO as string,",
				"          {IMPORTE EUR} as double,",
				"          SALDO as double",
				"     ),",
				"     errors: true) ~> FormatColumns",
				"FormatColumns select(mapColumn(",
				"          DateOperation = {FECHA OPERACIÓN},",
				"          DateEffective = {FECHA VALOR},",
				"          Description = CONCEPTO,",
				"          Amount = {IMPORTE EUR},",
				"          Saldo = SALDO",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenameColumns",
				"AddSource derive(Key = ERROR_FUNCTION('')) ~> AddKey",
				"Source sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DateOperation as string,",
				"          DateEffective as string,",
				"          Description as string,",
				"          Amount as string,",
				"          Saldo as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}